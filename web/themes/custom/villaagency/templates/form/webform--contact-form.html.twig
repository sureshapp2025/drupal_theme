<form id="contact-form{{ element['#element_id']}}" action="{{ element.action }}" method="{{ element.method }}">
		<div class="row">
			<div class="col-lg-12">
				<fieldset>
					<label for="name">{{ element.first_name.label }}</label>
					{{ element.elements.first_name }}
				</fieldset>
			</div>
			<div class="col-lg-12">
				<fieldset>
					<label for="email">{{ element.email.label }}</label>
					{{ element.elements.email }}
				</fieldset>
			</div>
			<div class="col-lg-12">
				<fieldset>
					<label for="subject">{{ element.last_name.label }}</label>
					{{ element.elements.last_name }}
				</fieldset>
			</div>
			<div class="col-lg-12">
				<fieldset>
					<label for="message">{{ element.message.label }}</label>
					{{ element.elements.message }}
				</fieldset>
			</div>
			<div class="col-lg-12">
				<fieldset>
					{{ element.submit }}
				</fieldset>
			</div>
		</div>
	</form>
